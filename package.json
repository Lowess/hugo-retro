{
  "name": "exampleSite",
  "version": "1.0.0",
  "type": "module",
  "description": "Hugo Preact - Example Site",
  "repository": "Lowess/hugo-preact-theme",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./assets/js",
    "lint:fix": "eslint ./assets/js --fix",
    "dev": "vite build && run-p dev:**",
    "dev:vite-watch": "vite build --watch --mode development",
    "dev:hugo": "hugo serve -d public -vw --bind ${HUGO_BIND:-127.0.0.1} -p ${HUGO_PORT:-3001} --baseURL ${BASE_URL:-http://localhost:3001} --appendPort=false",
    "start": "npm run dev",
    "preview": "hugo serve -d public --bind ${HUGO_BIND:-127.0.0.1} -p ${HUGO_PORT:-3001} --baseURL ${BASE_URL:-http://localhost:3001} --appendPort=false -D -F",
    "prebuild": "rm -rf public",
    "build": "vite build && hugo -d public -v -b ${BASE_URL:-/}",
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@preact/preset-vite": "^2.8.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "ejs": "^3.1.9",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-glob": "^3.3.2",
    "hugo-bin-extended": "^0.121.2",
    "inquirer": "^9.2.12",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "path": "^0.12.7",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^3.4.0",
    "toml": "^3.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  },
  "dependencies": {
    "@testing-library/preact": "^3.2.3",
    "antd": "^5.12.8",
    "preact": "^10.19.3",
    "preact-island": "^1.2.0",
    "preact-render-to-string": "^6.3.1",
    "react": "npm:@preact/compat@^17.1.2",
    "react-dom": "npm:@preact/compat@^17.1.2",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.2.1"
  },
  "resolutions": {
    "natives": "1.1.6"
  }
}
